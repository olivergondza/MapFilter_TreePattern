<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE patterns SYSTEM "TreePattern053.dtd">
<patterns>

  <pattern>
    <some>
      <one>
        <node_attr attr="north" attachPattern="distance" />
        <node_attr attr="south" attachPattern="distance" />
      </one>
      <one>
        <node_attr attr="east" attachPattern="distance" />
        <node_attr attr="west" attachPattern="distance" />
      </one>
    </some>
  </pattern>

  <pattern name="distance">
    <all>
      <attr valuePattern="\d*">count</attr>
      <attr valuePattern="meter|yard">unit</attr>
    </all>
  </pattern>

</patterns>
