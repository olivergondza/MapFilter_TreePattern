<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE patterns SYSTEM "TreePattern053.dtd">
<patterns>
  <pattern>
    <node_attr attr="steps" iterator="yes">
      <one>
        <attr valuePattern="left|right">turn</attr>
        <node_attr attr="walk">
          <one>
            <attr valuePattern="\d*">metres</attr>
            <attr valuePattern="\d*">yards</attr>
          </one>
        </node_attr>
      </one>
    </node_attr>
  </pattern>
</patterns>
