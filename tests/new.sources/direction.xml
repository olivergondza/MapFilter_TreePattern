<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE patterns SYSTEM "TreePattern053.dtd">
<patterns>

  <pattern>
    <!-- TreePattern_Direction__ -->
    <some>
      <one>
        <key name="top" attachPattern="distance" />
        <key name="bottom" attachPattern="distance" />
      </one>
      <one>
        <key name="right" attachPattern="distance" />
        <key name="left" attachPattern="distance" />
      </one>
    </some>
    <!-- __TreePattern_Direction -->
  </pattern>

  <pattern name="distance">
    <all>
      <key name="count">
        <value pattern="/^\d+$/" />
      </key>
      <key name="unit">
        <value pattern="/^(?:meter|yard)$/" />
      </key>
    </all>
  </pattern>

</patterns>
